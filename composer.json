{
	"name": "lille-bitte/annotations",
	"type": "library",
	"description": "DocBlock Annotation Parser",
	"license": "BSD-3-Clause",
	"keywords": [
		"doc-comment",
		"doc-block",
		"parser",
		"doc-comment-annotation-parser",
		"doc-block-annotation-parser"
	],
	"config": {
		"sort-packages": true
	},
	"autoload": {
